{
  "name": "karat-24",
  "version": "0.0.1",
  "description": "A starter for Medusa projects.",
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "sqlite",
    "postgres",
    "typescript",
    "ecommerce",
    "headless",
    "medusa"
  ],
  "scripts": {
    "build": "medusa build",
    "seed": "medusa exec ./src/scripts/seed.ts",
    "start": "NODE_ENV=production medusa start -H 0.0.0.0",
    "dev": "medusa develop -H 0.0.0.0",
    "dev:https": "concurrently -n MEDUSA,CADDY -c green,cyan \"npm run dev\" \"caddy run --config Caddyfile\"",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
  },
  "dependencies": {
    "@medusajs/admin-sdk": "^2.8.6",
    "@medusajs/admin-ui": "^2.1.18",
    "@medusajs/cli": "^2.8.6",
    "@medusajs/file": "^2.8.6",
    "@medusajs/file-local": "^2.12.1",
    "@medusajs/file-s3": "^2.12.1",
    "@medusajs/framework": "^2.8.6",
    "@medusajs/medusa": "^2.8.6",
    "@medusajs/pricing": "^2.8.6",
    "@medusajs/ui": "^4.0.19",
    "@mikro-orm/core": "6.4.3",
    "@mikro-orm/knex": "6.4.3",
    "@mikro-orm/migrations": "6.4.3",
    "@mikro-orm/postgresql": "6.4.3",
    "@types/multer": "^2.0.0",
    "awilix": "^8.0.1",
    "axios": "^1.12.2",
    "multer": "^2.0.2",
    "pg": "^8.13.0",
    "sharp": "^0.34.5",
    "slugify": "^1.6.6"
  },
  "devDependencies": {
    "@medusajs/test-utils": "2.8.6",
    "@mikro-orm/cli": "6.4.3",
    "@swc/core": "1.5.7",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.2.25",
    "concurrently": "^9.2.1",
    "jest": "^29.7.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.6.2",
    "vite": "^5.2.11",
